                             "/* boxclippure.fragment */\n"
                             "precision mediump float;\n"
                             "varying vec3 worldPosition;\n"
                             "uniform vec4 color;\n"
                             "uniform vec3 clip[2];\n"
                             "\n"
                             "uniform float intensity; // = 5.0;\n"
                             "\n"
                             "void main() \n"
                             "{\n"
                             "	gl_FragColor = color;\n"
                             "	#if 0\n"
