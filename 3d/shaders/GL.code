                             "		else\n"
                             "		{\n"
                             "			float beta = 2 * PI * r;\n"
                             "			float x_ = 0.5 + 0.5 * alpha / PI;\n"
                             "			float y_= 1 - beta / PI;\n"
                             "			zaiTC = vec2(x_, y_);\n"
                             "		}\n"
                             "	}\n"
                             "	\n"
                             "	return zaiTC;\n"
                             "}\n"
